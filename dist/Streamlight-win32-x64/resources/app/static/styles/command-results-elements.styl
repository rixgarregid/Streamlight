.command-panel
    display none
    padding 20px

.command-result
    height 30px
    line-height 30px
    padding 10px 20px !important
    color active-blue-color !important
    position relative
    cursor pointer
    font-size 13px
    font-family 'Roboto'

    span
        position absolute
        top 50%
        transform translateY(-50%)
        cursor pointer
        font-size 13px
        font-family 'Roboto'
