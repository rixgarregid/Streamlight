@font-face
  font-family 'icons'
  src url(icomoon-font-path + '/icons.eot?q1sc4n')
  src url(icomoon-font-path + '/icons.eot?q1sc4n#iefix') format('embedded-opentype'), url(icomoon-font-path + '/icons.ttf?q1sc4n') format('truetype'), url(icomoon-font-path + '/icons.woff?q1sc4n') format('woff'), url(icomoon-font-path + '/icons.svg?q1sc4n#icons') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'icons' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

icon
  width 20px
  height 20px
  line-height 20px
  text-align center
  font-size font-size-small

